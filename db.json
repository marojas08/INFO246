fetch('https://jsonplaceholder.typicode.com/items', {
  method: 'POST',
  body: JSON.stringify({
    title: 'laptop',
    body: 'bar',
    userId: 1,
  }),
  headers: {
    'Content-type': 'application/json; charset=UTF-8',
  },
})
  .then((response) => response.json())
  .then((json) => console.log(json));
